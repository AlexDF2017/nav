<xml>
  <report name="netbox">
    <title>Utstyrsregister</title>
    <query>SELECT roomid,sysname,host(ip),catid,subcat,typeid,typename,orgid,count(moduleid) AS antmod,netboxid,prefixid FROM type RIGHT OUTER JOIN netbox USING (typeid) LEFT JOIN module USING (netboxid) GROUP by netboxid,prefixid,sysname,ip,catid, subcat,typeid,typename,roomid,orgid</query>
    <sort>roomix,netboxid</sort>
    <hide>netboxid,prefixid,typeid</hide>
    <extra>mem,swp,gwp,kart</extra>
    <fields>
      <field name="sysname">
	<title>Navn</title>
      </field>
      <field name="catid">
	<title>Kat</title>
	<uri>netbox?catid=$catid</uri>
      </field>
      <field name="subcat">
	<title>Underkat</title>
      </field>
      <field name="ip">
	<title>IP-adresse</title>
	<uri>prefix?prefixid=$prefixid</uri>
      </field>
      <field name="typename">
	<title>Type</title>
	<uri>type?typeid=$sysname</uri>
      </field>
      <field name="roomid">
	<title>Rom</title>
	<uri>room?roomid=$roomid</uri>
      </field>
      <field name="orgid">
	<title>Drifter</title>
	<uri>org?orgid=$orgid</uri>
      </field>
      <field name="ip">
	<title>IP-adresse</title>
      </field>
      <field name="antmod">
        <uri>modules?sysname=$sysname</uri>
      </field>
      <field name="mem">
        <uri>mem?sysname=$sysname</uri>
	<explain>Minne</explain>
      </field>
      <field name="swp">
        <uri>swport?netboxid=$netboxid</uri>
	<explain>Svitsjeporter (om aktuelt)</explain>
      </field>
      <field name="gwp">
        <uri>gwport?netboxid=$netboxid</uri>
	<explain>Ruterporter (om aktuelt)</explain>
      </field>
      <field name="kart">
        <uri>/vlanPlot/?netboxid=$netboxid</uri>
      </field>      
    </fields>
  </report>
</xml>