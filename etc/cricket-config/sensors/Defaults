#
# Default Cricket config for sensors
#

datasource --default--
    rrd-ds-type  =    GAUGE
    rrd-min      =    undef
    rrd-max      =    undef

datasource sensor
    ds-source    =    snmp://%snmp%/%oid%

graph --default--
    draw-as      =    LINE2
    legend       =    %legend%
    y-axis       =    %yaxis%
    scale        =    %scalevalue%,/

targetType sensor
    ds           =    sensor
